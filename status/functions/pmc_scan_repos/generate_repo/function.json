{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "30 8 * * *"
    },
    {
      "name": "repositorystatus",
      "type": "blob",
      "dataType": "string",
      "path": "dynamic-public-data/repository_status.json",
      "connection": "pmcstatusprimary_CONNECTION",
      "direction": "in"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "repo-request-queue",
      "connection": "pmcstatusprimary_CONNECTION"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msgresultsqueue",
      "queueName": "results-queue",
      "connection": "pmcstatusprimary_CONNECTION"
    }
  ]
}