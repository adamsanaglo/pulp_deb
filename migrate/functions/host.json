{
  "version": "2.0",
  "functionTimeout": "01:00:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "extensions": {
      "serviceBus": {
          "maxConcurrentCalls": 1,
          "maxAutoLockRenewalDuration": "01:00:00",
          "prefetchCount": 20,
      }
  }
}
